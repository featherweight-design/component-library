{
  "name": "@f-design/component-library",
  "version": "1.0.2-HartiganHM-canary.4",
  "description": "A component library built and maintained by Featherweight Design",
  "main": "dist/bundle.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/featherweight-design/component-library#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/featherweight-design/component-library.git"
  },
  "bugs": {
    "url": "https://github.com/featherweight-design/component-library/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [],
  "author": "Featherweight Design",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "npx webpack",
    "postbuild": "mkdir ./dist/scss && cp -a ./src/styles/. ./dist/scss",
    "build-storybook": "build-storybook",
    "clean": "rm -rf node_modules yarn.lock && yarn install",
    "format": "prettier --write './src/**/*.{ts,tsx,mdx}'",
    "lint": "eslint './src/**/*.{js,ts,tsx,mdx}' --fix",
    "lint-ci": "eslint './src/**/*.{js,ts,tsx,mdx}'",
    "release": "standard-version -t @f-design/component-library@",
    "release-beta": "yarn release --prerelease beta --skip.changelog --skip.tag",
    "release-canary": "yarn release --prerelease $PR_DEV-canary --skip.changelog --skip.tag",
    "storybook": "start-storybook -p 6006 -s ./.storybook/public",
    "storybook-smoke-test": "start-storybook --smoke-test",
    "test": "jest --config jestconfig.json",
    "upgrade": "yarn upgrade-interactive --latest",
    "version": "yarn run format && git add -A src"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "core-js": "^3.5.0",
    "husky": "^3.1.0",
    "lodash": "^4.17.15",
    "regenerator-runtime": "^0.13.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-storyshots": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/storybook__addon-storyshots": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.3.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-mdx": "^1.7.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^24.9.0",
    "node-sass": "^5.0.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-test-renderer": "^16.12.0",
    "require-context.macro": "^1.2.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "standard-version": "^8.0.0",
    "style-loader": "^1.0.1",
    "terser-webpack-plugin": "^2.3.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "yarn run release-canary",
      "pre-push": "yarn run lint-ci"
    }
  }
}
